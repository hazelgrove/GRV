digraph G {
  rankdir=LR
  compound=true
  splines=true
  concentrate=true

  node [shape=box]

  Edg [label="Edge"]
  Uuid

  edge [arrowhead="vee"]

  subgraph cluster_graph {
    label="Graph.t"

    node [shape=none]

    edges; states; cache
  }

  Edge_state -> {edges; states} [dir=back]

  edges -> Uuid
  {edges; states} -> Edg
  cache -> Cache
}
