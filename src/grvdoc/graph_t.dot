digraph G {
  rankdir=LR
  compound=true
  splines=true
  concentrate=true

  node [shape=box]

  Edg [label="Edge"]
  Uuid

  edge [arrowhead="vee"]

  subgraph cluster_graph {
    label="Graph.t"

    node [shape=none]

    edges; states; cache
  }

  Uuid -> edges [dir=back]
  Edg -> {edges; states} [dir=back]
  {edges; states} ->  Edge_state
  cache -> Cache
}
